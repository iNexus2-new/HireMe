@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no, user-scalable=no">

    <meta name="author" content="GrandJob">
    <meta name="description" content="@ViewData["Description"]">
    <meta name="keywords" content="@ViewData["Keywords"]">
    <meta charset="UTF-8">
    <title>@ViewData["Title"] - GrandJob</title>

    <!-- Favicon-->
    <link rel="apple-touch-icon" sizes="180x180" href="~/Home/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Home/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Home/favicon/favicon-16x16.png">


    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet" type='text/css' />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700&display=swap" rel="stylesheet">

    <link href="~/panel/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/panel/assets/css/demo.css" rel="stylesheet" />

    <link href="~/panel/assets/css/light-bootstrap-dashboard.css" rel="stylesheet" />
    <link href="~/panel/assets/css/pe-icon-7-stroke.css" rel="stylesheet" />

</head>


<body>
    @RenderBody()

    <script src="~/Home/js/jquery-3.4.1.min.js"></script>
    <script src="https://use.fontawesome.com/9d3c696734.js"></script>

    <script src="~/panel/assets/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/panel/assets/js/light-bootstrap-dashboard.js?v=1.4.0"></script>
    <script src="~/panel/assets/js/demo.js"></script>
    <script src="https://apis.google.com/js/platform.js" async defer></script>

    @RenderSection("Scripts", required: false)
    @await Component.InvokeAsync("NToastNotify")
</body>
</html>

