@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no, user-scalable=no">
    <meta name="google-site-verification" content="f0HYNhGYM5QqqlHDnWyGqShec7YdmF9v939hd3ACPAE" />
    <meta name="author" content="GrandJob">
    <meta name="description" content="@ViewData["Description"]">
    <meta name="keywords" content="@ViewData["Keywords"]">
    <meta charset="UTF-8">

    <title>@ViewData["Title"] - GrandJob</title>

    <!-- Favicon-->
    <link rel="apple-touch-icon" sizes="180x180" href="~/Home/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Home/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Home/favicon/favicon-16x16.png">


    <link rel='stylesheet' href='~/customCSS/Base/font-awesome5.min.css'>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/customCSS/Base/base.css">
    <link rel="stylesheet" href="~/bootstrap/bootstrap.min.css">

    <link href="~/Home/css/ionicons.min.css" rel="stylesheet">
    <link href="~/Home/css/flaticon.css" rel="stylesheet">
    <link href="~/Home/css/icomoon.css" rel="stylesheet">

    <link href="~/customCSS/Core/aos.css" rel="stylesheet">
    <link href="~/customCSS/Core/animate.css" rel="stylesheet">

</head>


<body>
    @RenderBody()

    <script src="https://use.fontawesome.com/9d3c696734.js"></script>

    <script src="~/Home/js/jquery-3.4.1.min.js"></script>
    <script src="~/Home/js/owl.carousel.min.js"></script>
    <script src="~/Home/js/jquery.waypoints.min.js"></script>
    <script src="~/Home/js/jquery.animateNumber.min.js"></script>

    <script src="~/bootstrap/bootstrap.min.js"></script>
    <script src="~/Home/js/aos.js"></script>

    <script src="~/Home/js/scrollax.min.js"></script>

    <script src="~/Home/js/main.js"></script>


    @RenderSection("Scripts", required: false)
    @await Component.InvokeAsync("NToastNotify")
</body>
        </html>
